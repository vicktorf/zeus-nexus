# Zeus Nexus Frontend Secrets Configuration
# Copy this file to .streamlit/secrets.toml and update with your actual values

# Google OAuth2 Configuration
[oauth]
GOOGLE_CLIENT_ID = "your-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET = "your-client-secret"
OAUTH_REDIRECT_URI = "https://zeus-ui-ac-agentic.apps.prod01.fis-cloud.fpt.com"

# Zeus Core API
[api]
ZEUS_API_URL = "https://zeus-ac-agentic.apps.prod01.fis-cloud.fpt.com"

# LLM Provider API Keys (Optional - can be configured in backend instead)
[llm]
OPENAI_API_KEY = ""
ANTHROPIC_API_KEY = ""
GOOGLE_AI_API_KEY = ""
